<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
	<referenceContainer name="page.wrapper">
		   <container name="home.slider" before="main.content">
			   <block class="Magento\Cms\Block\Block" name="slide_home8">
				   <arguments>
					   <argument name="block_id" xsi:type="string">slide-home8</argument>
				   </arguments>
			   </block>
		   </container>		   
			
		<block class="Magento\Cms\Block\Block" name="instagram" before="reward">
			<arguments>
				<argument name="block_id" xsi:type="string">instagram_feed</argument>
			</arguments>
		</block>			
		
		
		<block class="Magento\Cms\Block\Block" name="reward" before="form.subscribe">
			<arguments>
				<argument name="block_id" xsi:type="string">loyality-reward</argument>
			</arguments>
		</block>		
			
	   </referenceContainer>
		

   </body>
</page>
